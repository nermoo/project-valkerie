{
  "name": "reactcicd",
  "homepage": "https://nermoo.github.io/project-valkerie/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy:release": "echo 'Deploying for release' && npm run build && gh-pages -b main -d dist",
    "deploy:development": "echo 'Deploying for development' && npm run build && gh-pages -b dev -d dist",
    "deploy:github-pages": "echo 'Deploying for GitHub Pages' && npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "antd": "^5.10.0",
    "gh-pages": "^6.1.1",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "typescript": "^5.4.2",
    "vite": "^5.0.8"
  }
}
