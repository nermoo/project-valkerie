    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^5.0.8"
